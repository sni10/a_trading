########################################
# Пример локального .env для прототипа #
########################################

# Логическое окружение запуска (local/dev/prod и т.п.)
# По умолчанию: local
# APP_ENV=local

# Список символов через запятую.
# По умолчанию: BTC/USDT,ETH/USDT
# SYMBOLS=BTC/USDT,ETH/USDT

# Максимальное количество тиков за один запуск демо-конвейера.
# По умолчанию: 10
MAX_TICKS=100

# Пауза между тиками в секундах.
# По умолчанию: 0.2
TICK_SLEEP_SEC=0.2

# Интервалы обновления индикаторов (в количестве тиков).
# Значения по умолчанию: 1 / 3 / 5
# INDICATOR_FAST_INTERVAL=1
# INDICATOR_MEDIUM_INTERVAL=3
# INDICATOR_HEAVY_INTERVAL=5
